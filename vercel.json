{
    "version":2,
    "enc":{
        "MONGO_URI":"minist"
    },
    "builds":[
        {
            "src":"./src/main.ts",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/src/main.ts",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
        
    ],
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}